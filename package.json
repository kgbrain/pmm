{
  "name": "pmm",
  "version": "0.0.1",
  "description": "PostMortem Management Tool",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf dist && rm -rf .next",
    "test": "jest",
    "dev": "npx nodemon --watch server server/server.js",
    "build": "npm run clean && npx next build",
    "start": "NODE_ENV=production node server/server.js",
    "now:login": "npx now login",
    "now": "npm run build && npx now"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kgbrain/pmm.git"
  },
  "keywords": [
    "post",
    "mortem",
    "management",
    "24/7"
  ],
  "author": "KGBrain",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kgbrain/pmm/issues"
  },
  "homepage": "https://github.com/kgbrain/pmm#readme",
  "dependencies": {
    "next": "^9.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@material-ui/core": "^4.2.1",
    "@material-ui/styles": "^4.2.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "firebase": "^6.3.3",
    "firebase-admin": "^8.3.0",
    "isomorphic-unfetch": "^3.0.0",
    "session-file-store": "^1.3.1"
  },
  "devDependencies": {
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.5",
    "@types/express": "^4.17.0",
    "now": "^15.8.7",
    "typescript": "^3.5.3",
    "firebase-tools": "^7.2.2",
    "nodemon": "^1.19.1",
    "marked-terminal": "^3.2.0"
  }
}
